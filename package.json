{
  "name": "express_ts",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/bin/dev.js",
    "lint": "eslint dist/**/*.js",
    "format": "eslint dist/**/*.js --fix",
    "dev:start": "nodemon --legacy-watch -r tsconfig-paths/register src/bin/dev.ts",
    "dev:lint": "eslint src/**/*.ts",
    "dev:format": "eslint src/**/*.ts --fix",
    "dev:undo": "npx sequelize-cli db:migrate:undo:all",
    "dev:migrate": "npx sequelize-cli db:migrate",
    "dev:seed": "npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all ",
    "dev:generate-migration": "npx sequelize-cli migration:generate --name",
    "dev:generate-seed": "npx sequelize-cli seed:generate --name",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
